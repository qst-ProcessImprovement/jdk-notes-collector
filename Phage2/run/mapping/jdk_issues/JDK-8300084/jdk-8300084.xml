diff --git a/src/java.desktop/macosx/classes/com/apple/laf/AquaFileChooserUI.java b/src/java.desktop/macosx/classes/com/apple/laf/AquaFileChooserUI.java&lt;br/&gt;
index 5461e25f7af..1dfe7a606eb 100644&lt;br/&gt;
--- a/src/java.desktop/macosx/classes/com/apple/laf/AquaFileChooserUI.java&lt;br/&gt;
+++ b/src/java.desktop/macosx/classes/com/apple/laf/AquaFileChooserUI.java&lt;br/&gt;
@@ -593,6 +593,11 @@ public class AquaFileChooserUI extends FileChooserUI {&lt;br/&gt;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;return fApproveButton;&lt;br/&gt;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;}&lt;br/&gt;
&amp;nbsp;&lt;br/&gt;
+    @Override&lt;br/&gt;
+    public JButton getDefaultButton(JFileChooser fc) {&lt;br/&gt;
+        return getApproveButton(fc);&lt;br/&gt;
+    }&lt;br/&gt;
+&lt;br/&gt;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;public int getApproveButtonMnemonic(final JFileChooser fc) {&lt;br/&gt;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;return fSubPanel.getApproveButtonMnemonic(fc);&lt;br/&gt;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;}</comment>
